#: collective/twitter/portlets/profiles/default/portlets.xml
msgid "A portlet that will show a profile feed from Twitter"
msgstr "Un portlet para mostrar los tweets de un usuario en Twitter"

#: collective/twitter/portlets/profiles/default/portlets.xml
msgid "A portlet to show search results from Twitter"
msgstr "Un portlet para mostrar resultados de búsquedas en Twitter"

#: collective/twitter/portlets/twprofile.py:65
#: collective/twitter/portlets/twsearch.py:65
msgid "Header"
msgstr "Cabecera"

#: collective/twitter/portlets/twprofile.py:82
#: collective/twitter/portlets/twsearch.py:82
msgid "Maximum results"
msgstr "Resultados máximos"

#: collective/twitter/portlets/twprofile.py:87
#: collective/twitter/portlets/twsearch.py:87
msgid "Pretty dates"
msgstr "Fechas mejoradas"

#: collective/twitter/portlets/twsearch.py:74
msgid "Search string"
msgstr "Cadena de búsqueda"

#: collective/twitter/portlets/twprofile.py:78
#: collective/twitter/portlets/twsearch.py:78
msgid "Show avatars"
msgstr "Mostrar avatares"

#: collective/twitter/portlets/twprofile.py:88
#: collective/twitter/portlets/twsearch.py:88
msgid "Show dates in a pretty format (ie. '4 hours ago')."
msgstr "Mostrar fechas en un formato mejorado (ej. 'hace 4 horas')."

#: collective/twitter/portlets/twprofile.py:79
#: collective/twitter/portlets/twsearch.py:79
msgid "Show people's avatars."
msgstr "Mostrar los avatares de las personas."

#: collective/twitter/portlets/twprofile.pt:54
#: collective/twitter/portlets/twsearch.pt:54
msgid ""
"The Twitter account used to display content in this portlet appears to be "
"invalid"
msgstr ""
"La cuenta Twitter utilizada para mostrar el contenido en el portlet parece "
"inválida"

#: collective/twitter/portlets/twprofile.py:75
msgid ""
"The Twitter user you wish to get feed from (you can include or omit the "
"initial @)."
msgstr ""
"El usuario de Twitter del cual se quieren obtener los tweets (se puede "
"incluir u omitir el @ inicial)."

#: collective/twitter/portlets/twprofile.py:66
#: collective/twitter/portlets/twsearch.py:66
msgid "The header for the portlet. Leave empty for none."
msgstr "La cabecera del portlet. Dejar vacío para ninguna."

#: collective/twitter/portlets/twprofile.py:83
#: collective/twitter/portlets/twsearch.py:83
msgid "The maximum results number."
msgstr "El número máximo de resultados."

#: collective/twitter/portlets/twprofile.py:69
#: collective/twitter/portlets/twsearch.py:69
msgid "Twitter account"
msgstr "Cuenta de Twitter"

#: collective/twitter/portlets/profiles/default/portlets.xml
#: collective/twitter/portlets/twprofile.py:129
msgid "Twitter profile Portlet"
msgstr "Portlet de perfil Twitter"

#: collective/twitter/portlets/profiles/default/portlets.xml
#: collective/twitter/portlets/twsearch.py:129
msgid "Twitter search Portlet"
msgstr "Portlet de búsqueda en Twitter"

#: collective/twitter/portlets/twprofile.py:74
msgid "Twitter user"
msgstr "Usuario de Twitter"

#: collective/twitter/portlets/twsearch.py:75
msgid "What to search."
msgstr "Qué buscar"

#: collective/twitter/portlets/twprofile.py:70
#: collective/twitter/portlets/twsearch.py:70
msgid "Which twitter account to use."
msgstr "Qué cuenta de Twitter utilizar."


